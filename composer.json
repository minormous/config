{
    "name": "minormous/config",
    "description": "A library for configuring the Auryn dependency injector",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Minormous Contributors",
            "homepage": "https://github.com/minormous"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.3",
        "minormous/structure": "dev-master|^1",
        "rdlowrey/auryn": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Minormous\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MinormousTests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
